    "# Naive bayes\n",
    "import numpy as np\n",
    "X = np.array([[-1, -1], [-2, -4], [-4, -6], [1, 2]])\n",
    "Y = np.array([1, 1, 2, 1])\n",
    "from sklearn.naive_bayes import GaussianNB\n",
    "GNBclf = GaussianNB()\n",
    "GNBclf.fit(X, Y)"
